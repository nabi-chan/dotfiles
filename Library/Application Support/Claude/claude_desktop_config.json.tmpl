{
  "mcpServers": {
    "connector-mongo": {
      "type": "stdio",
      "command": "{{ .chezmoi.homeDir }}/.local/share/mise/shims/npx",
      "args": ["-y", "mongodb-mcp-server@latest"],
      "env": {
        "MDB_MCP_CONNECTION_STRING": "mongodb://root:1744@localhost:27017"
      }
    },
    "connector-mysql-local": {
      "type": "stdio",
      "command": "{{ .chezmoi.homeDir }}/.local/share/mise/shims/npx",
      "args": ["-y", "@benborla29/mcp-server-mysql@2.0.3"],
      "env": {
        "ALLOW_DELETE_OPERATION": "true",
        "ALLOW_INSERT_OPERATION": "true",
        "ALLOW_UPDATE_OPERATION": "true",
        "MULTI_DB_WRITE_MODE": "true",
        "MYSQL_DB": "",
        "MYSQL_HOST": "127.0.0.1",
        "MYSQL_PASS": "1744",
        "MYSQL_USER": "root"
      }
    },
    "documentation-aws": {
      "command": "uvx",
      "args": ["awslabs.aws-documentation-mcp-server@latest"],
      "env": {
        "AWS_DOCUMENTATION_PARTITION": "aws"
      }
    },
    "documentation-context-7": {
      "type": "stdio",
      "command": "{{ .chezmoi.homeDir }}/.local/share/mise/shims/npx",
      "args": ["-y", "mcp-remote@latest", "https://mcp.context7.com/mcp"]
    },
    "documentation-shadcn": {
      "type": "stdio",
      "command": "{{ .chezmoi.homeDir }}/.local/share/mise/shims/npx",
      "args": ["-y", "shadcn@latest", "mcp"]
    },
    "tools-figma": {
      "type": "stdio",
      "command": "{{ .chezmoi.homeDir }}/.local/share/mise/shims/npx",
      "args": ["-y", "mcp-remote@latest", "http://127.0.0.1:3845/mcp"]
    },
    "tools-pyhub": {
      "type": "stdio",
      "command": "{{ .chezmoi.homeDir }}/mcptools/pyhub.mcptools/pyhub.mcptools",
      "args": ["run", "stdio"]
    }
  },
  "preferences": {
    "menuBarEnabled": false,
    "quickEntryShortcut": "double-tap-option"
  }
}
