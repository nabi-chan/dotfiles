{
  "[prisma]": {
    "editor.defaultFormatter": "Prisma.prisma"
  },
  "cursor.composer.shouldChimeAfterChatFinishes": true,
  "cursor.cpp.enablePartialAccepts": true,
  "diffEditor.ignoreTrimWhitespace": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontFamily": "'JetBrainsMono Hangul Nerd Font', 'D2Coding', monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 14,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "editor.guides.bracketPairs": true,
  "editor.inlineSuggest.enabled": true,
  "editor.largeFileOptimizations": true,
  "editor.linkedEditing": true,
  "editor.mouseWheelZoom": true,
  "editor.quickSuggestions": {
    "other": "on",
    "strings": "on"
  },
  "editor.stickyScroll.enabled": true,
  "editor.tabSize": 2,
  "emmet.triggerExpansionOnTab": true,
  "emmet.useInlineCompletions": true,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmPasteNative": false,
  "extensions.gallery.serviceUrl": "https://marketplace.visualstudio.com/_apis/public/gallery",
  "files.associations": {
    "*.css": "tailwindcss",
    "*.json.tmpl": "jsonl",
    "*.just": "shellscript",
    "*.toml.local": "toml"
  },
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.ignoreMissingGitWarning": false,
  "git.openRepositoryInParentFolders": "always",
  "githubPullRequests.fileListLayout": "flat",
  "githubPullRequests.pullBranch": "never",
  "importCost.showCalculatingDecoration": false,
  "importCost.typescriptExtensions": ["\\.ts$", "\\.tsx?$"],
  "java.configuration.detectJdksAtStart": true,
  "java.configuration.runtimes": [
    {
      "name": "JavaSE-21",
      "path": "/Library/Java/JavaVirtualMachines/21.0.2.jdk/Contents/Home"
    },
    {
      "name": "JavaSE-23",
      "path": "/Library/Java/JavaVirtualMachines/23.0.2.jdk/Contents/Home"
    }
  ],
  "java.configuration.updateBuildConfiguration": "automatic",
  "java.jdt.ls.vmargs": "-XX:+UseParallelGC -XX:GCTimeRatio=4 -XX:AdaptiveSizePolicyWeight=90 -Dsun.zip.disableMemoryMapping=true -Xmx32G -Xms512M -Xlog:disable",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "prettier.enable": true,
  "prettier.resolveGlobalModules": true,
  "redhat.telemetry.enabled": false,
  "security.promptForLocalFileProtocolHandling": false,
  "tailwindCSS.classFunctions": ["cva", "clsx", "cx", "cn"],
  "terminal.explorerKind": "external",
  "terminal.integrated.enableMultiLinePasteWarning": "never",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "window.confirmSaveUntitledWorkspace": true,
  "workbench.colorTheme": "Catppuccin Frapp√©",
  "workbench.editor.enablePreviewFromQuickOpen": true,
  "workbench.iconTheme": "catppuccin-frappe"
}
